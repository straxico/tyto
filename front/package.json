  "engines": {
    "node": "12.x",
    "npm": ">= 6.9.0",
    "yarn": ">=1.15.0"
  },
  "scripts": {
    "start": "yarn start-clean && node --icu-data-dir=node_modules/full-icu --require dotenv/config config/server/index.js dotenv_config_path=./config/env/.env-dev",
    "server": " node --icu-data-dir=node_modules/full-icu --require dotenv/config config/server/index.js dotenv_config_path=./config/env/.env-prod",
    "storybook": "start-storybook -c ./config/storybook -s ./public, -p 9001",
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "yarn && yarn clean && yarn build:icon && yarn build:fontIcons && yarn build:next ",
    "build:next": "next build",
    "build:icon": "babel-node config/font/build.js",
    "build:fontIcons": "babel-node config/font/createSVGFont.js",
    "build:help": "babel-node config/helpfile.js",
    "build-storybook": "build-storybook -c ./config/storybook -s ./public  -o storybook",
    "test": "jest -u --config config/test/jest.config.js",
    "lint": "eslint src/**/*.{jsx,tsx,ts,js} --fix",
    "start-clean": "rimraf .next/server/static .next/static/development ",
    "clean": "rimraf .next/* src/Components/Atoms/Icons ",
    "proxy": "lcp --proxyUrl http://jajiga.com --proxyPartial px --port 8002"
  },
